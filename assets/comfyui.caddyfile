https://localhost:8189 {
    @static {
        path /static/* /assets/* /**/*.js /**/*.css /**/*.woff2
    }

    handle @static {
        reverse_proxy localhost:8188
        cache {
            ttl 1h
        }

        encode {
            br {
                level 9
            }
            gzip {
                level 9
            }
        }
    }

    reverse_proxy localhost:8188
    encode {
        gzip {
            level 9
        }
    }
}
